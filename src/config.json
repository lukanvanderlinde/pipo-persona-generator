{
  "palette": {
    "primary": {
      "main": "#6442fb"
    },
    "secondary": {
      "main": "#0099ff"
    },
    "error": {
      "main": "#ea202c"
    }
  },
  "typography": {
    "h1": {
      "fontFamily": [
        "TruenoBlk",
        "TruenoSBd",
        "Arial",
        "sans-serif",
        "\"Apple Color Emoji\"",
        "\"Segoe UI Emoji\"",
        "\"Segoe UI Symbol\""
      ],
      "color": "#36393d",
      "fontSize": "3.6rem",
      "src": `url(${TruenoBlk}) format('otf'), url(${TruenoSBd}) format('otf')`
    },
    "h2": {
      "fontFamily": [
        "TruenoBlk",
        "TruenoSBd",
        "Arial",
        "sans-serif",
        "\"Apple Color Emoji\"",
        "\"Segoe UI Emoji\"",
        "\"Segoe UI Symbol\""
      ],
      "color": "#36393d",
      // fontSize: '2.6rem',
      "textTransform": "uppercase",
      "src": `url(${TruenoBlk}) format('otf'), url(${TruenoSBd}) format('otf')`
    },
    "h3": {
      "fontFamily": [
        "TruenoRg",
        "Arial",
        "sans-serif",
        "\"Apple Color Emoji\"",
        "\"Segoe UI Emoji\"",
        "\"Segoe UI Symbol\""
      ],
      "color": "#747c8f",
      // fontSize: '2.2rem',
      "src": `url(${TruenoRg}) format('otf')`
    },
    "h4": {
      "fontFamily": [
        "TruenoRg",
        "Arial",
        "sans-serif",
        "\"Apple Color Emoji\"",
        "\"Segoe UI Emoji\"",
        "\"Segoe UI Symbol\""
      ],
      "color": "#36393d",
      // fontSize: '1.6rem',
      "src": `url(${TruenoRg}) format('otf')`
    },
    "body1": {
      "fontFamily": [
        "TruenoRg",
        "Arial",
        "sans-serif",
        "\"Apple Color Emoji\"",
        "\"Segoe UI Emoji\"",
        "\"Segoe UI Symbol\""
      ],
      "color": "#36393d",
      // fontSize: '2rem',
      "src": `url(${TruenoRg}) format('otf')`
    },
    "button": {
      "fontFamily": [
        "TruenoSBd",
        "Trueno",
        "Arial",
        "Helvetica",
        "sans- serif"
      ],
      // fontSize: '1.6rem',
      "color": "#36393d",
      "textTransform": "uppercase",
      "textDecoration": "none",
      "src": `url(${TruenoSBd}) format('otf')`
